{
  "name": "agora-backend",
  "version": "1.0.0",
  "description": "Ethereum dApp backend with AI Oracle for winner determination",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "encrypt-key": "ts-node scripts/encryptPrivateKey.ts",
    "test:e2e": "ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:mock": "NODE_ENV=test ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:real": "NODE_ENV=production ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:committee": "USE_COMMITTEE=true NODE_ENV=test ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:sports": "SCENARIO=sports NODE_ENV=test ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:crypto": "SCENARIO=crypto NODE_ENV=test ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:election": "SCENARIO=election NODE_ENV=test ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:interactive": "ts-node scripts/e2e-oracle-test.ts",
    "test:e2e:batch": "NODE_ENV=test ts-node -e \"const scenarios=['sports','crypto','election','climate','oscars']; scenarios.forEach(s => require('child_process').execSync('SCENARIO='+s+' npm run test:e2e:mock', {stdio:'inherit'}));\"",
    "test:e2e:auto-mock": "NODE_ENV=test npx ts-node scripts/run-e2e-test-automated.ts",
    "test:e2e:auto-real": "NODE_ENV=production npx ts-node scripts/run-e2e-test-real.ts",
    "railway:local": "railway run npm start",
    "railway:dev": "railway run npm run dev", 
    "railway:check": "node scripts/check-railway-env.js",
    "railway:variables": "railway variables",
    "railway:logs": "railway logs",
    "railway:status": "railway status"
  },
  "keywords": [
    "ethereum",
    "oracle",
    "ai",
    "blockchain"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google/generative-ai": "^0.24.1",
    "async-mutex": "^0.4.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "openai": "^4.24.1",
    "reflect-metadata": "^0.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
